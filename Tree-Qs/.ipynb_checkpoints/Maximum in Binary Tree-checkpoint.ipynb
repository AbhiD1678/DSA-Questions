   "source": [
    "import math\n",
    "\n",
    "def getMax(root):\n",
    "    if root==None:\n",
    "        return -math.inf\n",
    "    else:\n",
    "        ls=getMax(root.left)\n",
    "        rs=getMax(root.right)\n",
    "        return max(root.key,ls,rs)"
   ]
  },
