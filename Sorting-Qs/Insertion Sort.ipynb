    "# Time complexity:O(n^2)\n",
    "# in place and stable algorithm\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "6631aba0",
   "metadata": {},
   "outputs": [],
   "source": [
    "def InsertionSort(l):\n",
    "    for i in range(1,len(l)):\n",
    "        x=l[i]\n",
    "        j=i-1\n",
    "        while j>=0 and x <l[j]:\n",
    "            l[j+1]=l[j]\n",
    "            j=j-1\n",
    "        l[j+1]=x"
   ]
