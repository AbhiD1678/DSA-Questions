   "source": [
    "def Bsearch(lst,x,low,high):\n",
    "    if low>high:\n",
    "        return -1\n",
    "    mid=(low+high)//2\n",
    "    if x==lst[mid]:\n",
    "        return mid\n",
    "    elif x>lst[mid]:\n",
    "        return Bsearch(lst,x,mid+1,high)\n",
    "    elif x<lst[mid]:\n",
    "        return Bsearch(lst,x,low,mid-1)"
   ]
  }
