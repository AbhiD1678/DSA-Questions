   "source": [
    "def SubString(st,curr='',ind=0):\n",
    "    if ind==len(st):\n",
    "        print(curr,end=' ')\n",
    "        return\n",
    "    SubString(st,curr,ind+1)\n",
    "    SubString(st,curr+st[ind],ind+1)\n",
    "\n",
    "#Time complexity:O(2^n)\n",
    "#Space complexity:O(n)"
   ]
  },
