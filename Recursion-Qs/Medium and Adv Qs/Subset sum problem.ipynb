   "source": [
    "def SubsetSum(arr,n,sum):\n",
    "    if n==0:\n",
    "        return 1 if sum==0 else  0\n",
    "    return SubsetSum(arr,n-1,sum)+SubsetSum(arr,n-1,sum-arr[n-1])\n",
    "\n",
    "#Time complexity:O(n)"
   ]
  },
