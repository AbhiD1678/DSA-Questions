    "def isSeq(s1,s2):\n",
    "    i,j=0,0\n",
    "    \n",
    "    while i<len(s1) and j<len(s2):\n",
    "        if s1[i]==s2[j]:\n",
    "            j=j+1\n",
    "        i=i+1\n",
    "        \n",
    "    if j==len(s2):\n",
    "        return True\n",
    "    else:\n",
    "        return False"
   ]
