   "source": [
    "s=input(\"Enter the input:\")\n",
    "low=0\n",
    "high=len(s)-1\n",
    "\n",
    "while low<high:\n",
    "    if s[low]!=s[high]:\n",
    "        print(\"No,it's not a palindrome\")\n",
    "        break\n",
    "    low+=1\n",
    "    high-=1\n",
    "\n",
    "else:\n",
    "    print(\"It's a palindrome\")"
   ]
