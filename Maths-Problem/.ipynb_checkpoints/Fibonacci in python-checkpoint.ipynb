   "source": [
    "# Palindrome try\n",
    "\n",
    "arr1=[0,1]\n",
    "\n",
    "n=int(input('Enter the number of sequence:'))\n",
    "\n",
    "for i in range(n-2):\n",
    "    num=arr1[i]+arr1[i+1]\n",
    "    arr1.append(num)\n",
    "\n",
    "print('The fibonacci sequence is: ')\n",
    "for num in arr1:\n",
    "    print(num)"
