   "source": [
    "def Ipow(x,n):\n",
    "    \n",
    "    res=1\n",
    "    while n>0:\n",
    "        if n%2!=0:\n",
    "            res=res*x\n",
    "        \n",
    "        x=x*x\n",
    "        n=n//2\n",
    "    return res\n",
    "    "
   ]
