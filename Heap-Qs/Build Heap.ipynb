   "source": [
    "class MyMinHeap:\n",
    "    def __init__(self,l=[]):\n",
    "        self.arr=l\n",
    "        i=(len(l)-2)//2\n",
    "        while i>=0:\n",
    "            self.minHeapify(i)\n",
    "            i=i-1"
   ]
  }
