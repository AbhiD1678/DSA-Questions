   "source": [
    "def GivenSum(arr,s):\n",
    "\n",
    "    for i in range(len(arr)):\n",
    "        subSum=0\n",
    "        for j in range(i,len(arr)):\n",
    "            subSum+=arr[j]\n",
    "            if subSum==s:\n",
    "                return True\n",
    "    return False\n",
    "\n",
    "# Time complexity:O(n^2)"
   ]
  },
