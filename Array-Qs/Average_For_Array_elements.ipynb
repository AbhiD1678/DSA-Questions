    "#M1 normal method\n",
    "def averag(l):\n",
    "    sum1=0\n",
    "    for x in l:\n",
    "        sum1=sum1+x\n",
    "    n=len(l)\n",
    "    return sum1/n\n",
    "\n",
    "list1=[10,20,30,40]\n",
    "averag(list1)"
